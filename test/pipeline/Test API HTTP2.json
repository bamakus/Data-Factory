					{
						"activity": "Copy data API",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"sink": {
									"name": "Client_PK",
									"type": "int"
								}
							},
							{
								"sink": {
									"name": "ClientCode",
									"type": "varchar"
								}
							},
							{
								"sink": {
									"name": "Client",
									"type": "varchar"
								}
							},
							{
								"sink": {
									"name": "TypeClient",
									"type": "varchar"
								}
							},
							{
								"sink": {
									"name": "SegmentationClient",
									"type": "varchar"
								}
							},
							{
								"sink": {
									"name": "DateDebut",
									"type": "datetime"
								}
							},
							{
								"sink": {
									"name": "DateFin",
									"type": "datetime"
								}
							},
							{
								"sink": {
									"name": "IsRowCurrent",
									"type": "int"
								}
							},
							{
								"sink": {
									"name": "LastUpdated",
									"type": "datetime"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "AzureBlob3",
						"type": "DatasetReference",
						"parameters": {
							"filename": "json_http2.csv"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable2",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"token": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Http"
		},
		"annotations": []
