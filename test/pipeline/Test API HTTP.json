{
	"name": "Test API HTTP",
	"properties": {
		"activities": [
			{
				"name": "Copy Data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						}
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								}
							},
							{
								"source": {
									"path": "$['titre']"
								}
							},
							{
								"source": {
									"path": "$['contenu']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "HttpJson",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlob3",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Http"
		},
		"annotations": []
	}
}